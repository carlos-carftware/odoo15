<odoo>
    <data>
        <record id="ir_cron_active_subcription" model="ir.cron">
            <field name="name">CRON conciliar </field>
            <field name="model_id" ref="model_account_move"/>
            <field name="type">ir.actions.server</field>
            <field name="state">code</field>
            <field name="code">model.cron_payment_concillation()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
            <field name="priority" eval="50"/>
            <field name="nextcall" eval="(DateTime.now().replace(hour=4, minute=0) + timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')" />

        </record>
    </data>
</odoo>
